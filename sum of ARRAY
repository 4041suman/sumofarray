ublic class sumofarray {
    public static void main(String[] args) {
        int a[] = {4,5,6,8,9};
        int sum = 0;

        ///enhanced loop 
         for (int value : a){
            sum = sum + value;
        }


        //normal iteration
        for(int i=0;i<=4;i++){
            sum=sum+a[i];

        }
        System.out.println("Sum of the array: "+sum);
    }
